//Use multiple translations to reposition a series of shapes.

background(150);
size(500,500);

noStroke();
fill(200);
ellipse(250,250,100,100);

noStroke();
fill(220);
translate(50,50);
ellipse(250,250,120,120);

noStroke();
fill(240);
translate(50,50);
rect(250,250,50,50);

noStroke();
fill(255);
translate(-50,50);
rect(250,250,50,50);




